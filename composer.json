{
    "name": "donorservices/filament-notification-wizard",
    "description": "This is my package filament-notification-wizard",
    "keywords": [
        "donorservices",
        "laravel",
        "filament-notification-wizard"
    ],
    "homepage": "https://github.com/donorservices/filament-notification-wizard",
    "support": {
        "issues": "https://github.com/donorservices/filament-notification-wizard/issues",
        "source": "https://github.com/donorservices/filament-notification-wizard"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Azriel Walston",
            "email": "",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Donorservices\\FilamentNotificationWizard\\": "src/",
            "Donorservices\\FilamentNotificationWizard\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Donorservices\\FilamentNotificationWizard\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Donorservices\\FilamentNotificationWizard\\FilamentNotificationWizardServiceProvider"
            ],
            "aliases": {
                "FilamentNotificationWizard": "Donorservices\\FilamentNotificationWizard\\Facades\\FilamentNotificationWizard"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}